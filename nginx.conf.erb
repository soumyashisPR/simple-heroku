worker_processes 1;

events { worker_connections 1024; }

http {
    sendfile on;

    server {
        listen <%= ENV["PORT"] %>;



location / {
            return 200 'hi';
            add_header Content-Type text/plain;
        }




        # Custom block
        location /custom {
            return 200 'This is a custom NGINX block!';
            add_header Content-Type text/plain;
        }
    }
}

